<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.MainGameUI.MoveHistoryViewer">
   <children>
      <ListView fx:id="moveHistoryListView" layoutX="14.0" layoutY="14.0" prefHeight="330.0" prefWidth="572.0" />
      <Button fx:id="closeButton" layoutX="270.0" layoutY="360.0" mnemonicParsing="false" onAction="#handleClose" text="关闭">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="muteButton" contentDisplay="TOP" layoutX="550.0" layoutY="360.0" onAction="#handleMuteAction" style="-fx-background-color: transparent; -fx-cursor: hand; -fx-padding: 0;" />
   </children>
</AnchorPane>